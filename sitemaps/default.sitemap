sitemap default label="At Home"
{
    Frame label="Home"
    {
        Text item=Astro_Day_Phase label="Astro [%s]" icon="sun_clouds"
        {
            Text item=Astro_Sun_RiseNext
            Text item=Astro_Sun_SetNext
            Text item=Astro_Sun_Direction       visibility=[Astro_Sun_Elevation > 0]
            Text item=Astro_Sun_Elevation       visibility=[Astro_Sun_Elevation > 0]
            Text item=Astro_Moon_Phase          visibility=[Astro_Sun_Elevation <= 0]
            Text item=Astro_Moon_Distance       visibility=[Astro_Sun_Elevation <= 0]
            Text item=Astro_Moon_FullNext
            Text item=Astro_Moon_NewNext
        }

        Text item=Astro_Day_Time label="Climate [%1$tA, %1$td.%1$tm.%1$tY]" icon="sun_clouds"
        {
            Text item=Temp_Avg_Livingroom valuecolor=[>22="red",>20="green",>19="orange",<19="blue"]
            {
                Switch item=Climate_Period1 mappings=[TDY_30="Today", YDY_30="Yesterday", WK_1="Week", LWK_1="Wk-1", MNT_1D="Month", LMNT_1D="M-1"] label="" icon="calendar"
                Webview label="Climate Livingroom" url="/static/grafana.html?kiosk&dashboard=openhab&theme=dark&fromItem=Climate_Period1&panelItem=Climate_Panel1" height=12 icon="line"
            }
    
            Text item=NHC_CO2_Livingroom label="CO2 level livingroom [%.0f ppm]" valuecolor=[>1900="red",>1400="orange",<1400="green"]
            {
                Switch item=Climate_Period2 mappings=[TDY_30="Vandaag", YDY_30="Gisteren", WK_1="Week", LWK_1="Wk-1", MNT_1D="Maand", LMNT_1D="Mnd-1"] label="" icon="calendar"
                Webview label="Climate Livingroom" url="/static/grafana.html?kiosk&dashboard=openhab&theme=dark&fromItem=Climate_Period2&panelItem=Climate_Panel2" height=12 icon="line"
            }

            Text item=NHC_Humidity_Livingroom valuecolor=[>60="orange",>40="green",<=40="red"]
            {
                Switch item=Climate_Period1 mappings=[TDY_30="Vandaag", YDY_30="Gisteren", WK_1="Week", LWK_1="Wk-1", MNT_1D="Maand", LMNT_1D="Mnd-1"] label="" icon="calendar"
                Webview label="Climate Livingroom" url="/static/grafana.html?kiosk&dashboard=openhab&theme=dark&fromItem=Climate_Period1&panelItem=Climate_Panel1" height=12 icon="line"
                Text item=VS_GarageHum
                Text item=VS_CorridorHum
            }

            Text item=Weather_TempAverage_Outdoor label="Outdoor temperature [%.0f °C]"
            {
                Switch item=Climate_Period6 mappings=[DAY="Today", DAY_1="Yesterday", WEEK="This Week", WEEK_1="Last Week"]
                Webview label="Outdoor temperature left side of the house" url="/static/grafana.html?kiosk&dashboard=openhab&theme=dark&fromItem=Climate_Period6&panelItem=Climate_Panel6" height=12 icon="none"
            }

